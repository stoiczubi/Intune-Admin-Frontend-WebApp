{
  "name": "intune-admin-webapp",
  "version": "1.0.0",
  "description": "Intune MDM Management Web Application",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": ["intune", "microsoft-graph", "mdm"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@azure/identity": "^4.0.1",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "multer": "^1.4.5-lts.1",
    "csv-parser": "^3.0.0",
    "ws": "^8.14.2",
    "express-ws": "^5.0.2",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}

